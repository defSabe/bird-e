<h1>more bird info</h1>
<div>
    <div>
        <a href="/main-board">BACK</a>
        <h1>{{this.name}}</h1>
        <h3>{{this.scientificName}}</h3>
    </div>
    <div>
        <img src="{{this.imageUrl}}" alt="{{this.name}}" />
        <div>
            <h2>{{this.dateOfSight}}</h2>
            <h2>{{this.location}}</h2>
            <p>{{this.moreInfo}}</p>
            <div>
                <a href="/birds/{{this._id}}/edit">Edit</a>
                <a href="/birds/{{this._id}}/delete">Delete</a>
            </div>
        </div>
    </div>
</div>